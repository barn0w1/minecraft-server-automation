# Discord
DISCORD_TOKEN=
# Optional: to sync slash commands instantly to a specific guild (server).
GUILD_ID=

# mc-control (AWS Lambda Function URL)
MC_CONTROL_URL=

# Defaults
DEFAULT_WORLD=test
EPHEMERAL_DEFAULT=true

# Optional access control (comma-separated IDs)
# If ALLOWED_ROLE_IDS is set, only members with one of these roles can use commands.
ALLOWED_ROLE_IDS=
# If ALLOWED_USER_IDS is set, those users are always allowed.
ALLOWED_USER_IDS=
